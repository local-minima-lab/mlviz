# MLviz Backend Environment Variables
# Copy this file to .env for local development
# For production, set these in Google Cloud Run environment variables

# Application
DEBUG=true
APP_NAME=MLviz
VERSION=1.0.0

# Server
HOST=0.0.0.0
PORT=8000

# CORS - Frontend URL (production)
FRONTEND_URL=

# Additional allowed origins (comma-separated)
# ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Model cache
CACHE_ENABLED=true
CACHE_MAX_SIZE=100
